---
interface Props {
    title: string;
}

const { title } = Astro.props;
---

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />

    <title>{title}</title>
    <link rel="icon" type="image/png" href="/Butterfly3.png" />
    <meta property="og:title" content={title} />
    <meta property="og:site_name" content="Dalia Wadsworth" />

    <!-- load fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Reenie+Beanie&display=swap" rel="stylesheet">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Annie+Use+Your+Telescope&family=Borel&family=Butterfly+Kids&family=Londrina+Shadow&family=Moirai+One&family=Ribeye+Marrow&family=Shizuru&family=Yomogi&display=swap" rel="stylesheet">
    <link rel="sitemap" href="/sitemap-index.xml" />


</head>